    "@neolution-ch/javascript-utils": "^0.6.0",
    "@neolution-ch/next-with-google-secrets": "^0.2.1",
    "@neolution-ch/react-data-table": "^3.3.0",
    "@neolution-ch/react-hook-form-components": "^2.7.0",
    "@neolution-ch/react-pattern-ui": "^2.6.0",
    "@popperjs/core": "^2.11.6",
    "@types/cookie": "^0.6.0",
    "axios": "^1.3.4",
    "axios-auth-refresh": "^3.3.6",
    "axios-retry": "^3.4.0",
    "bootstrap": "^5.2.2",
    "classnames": "^2.3.2",
    "config": "^3.3.9",
    "cookie": "^0.6.0",
    "cookies-next": "^2.1.2",
    "date-fns": "^2.30.0",
    "http-proxy": "^1.18.1",
    "jquery": "^3.6.1",
    "next": "^14.2.3",
    "next-secure-headers": "^2.2.0",
    "nextjs-progressbar": "^0.0.16",

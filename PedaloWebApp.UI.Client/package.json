{
  "name": "react-temp-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "yarn node ./config/generateCustomEnvVariables.js && yarn orval && cross-env IS_BUILD=true next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:cleanup": "shx rm -rf .next && shx rm -rf dist && shx echo Build directory has been cleaned up",
    "buildandstart": "yarn build && yarn start",
    "check": "yarn install --check-files",
    "dev": "yarn install && yarn orval && next dev --experimental-https  -p 30349",
    "devo": " explorer http://localhost:30349 & yarn dev",
    "kill": "taskkill /im node.exe /F",
    "lint": "eslint \"**/*.{ts,tsx}\" --cache --max-warnings 0",
    "pr": "yarn lint && yarn build",
    "refreshVSToken": "npx vsts-npm-auth -config .npmrc",
    "start": "concurrently -n \"next,https\"  \"cross-env next start -p 3011\" \"npx local-ssl-proxy --key certificates/localhost-key.pem --cert certificates/localhost.pem --source 30349 --target 3011\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",

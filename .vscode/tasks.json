{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build Pedalo.UI.Api",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "-c",
        "Debug",
        "${workspaceFolder}/Pedalo.UI.Api/Pedalo.UI.Api.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build Pedalo.Tools.DataInitializer",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "-c",
        "Debug",
        "${workspaceFolder}/Pedalo.Tools.DataInitializer/Pedalo.Tools.DataInitializer.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
